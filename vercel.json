{
    "routes": [
      {
        "src": "/assets/(.*)\\.js",
        "headers": { "Content-Type": "application/javascript" },
        "dest": "/assets/$1.js"
      },
      {
        "src": "/assets/(.*)\\.css",
        "headers": { "Content-Type": "text/css" },
        "dest": "/assets/$1.css"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      { 
        "src": "/reset-password/(.*)", 
        "dest": "/index.html" 
      },
      { 
        "src": "/forgot-password", 
        "dest": "/index.html" 
      },
      { 
        "src": "/login", 
        "dest": "/index.html" 
      },
      { 
        "src": "/register", 
        "dest": "/index.html" 
      },
      { 
        "src": "/(.*)", 
        "dest": "/index.html" 
      }
    ]
}